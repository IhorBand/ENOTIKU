<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>@ViewBag.Title</title>

    <link href="https://fonts.googleapis.com/css?family=Raleway" rel="stylesheet" />

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jquery-validate")
    @Scripts.Render("~/bundles/jquery-cookie")
    @Scripts.Render("~/bundles/popper")
    @Scripts.Render("~/bundles/bootstrap")

    <script src="~/Content/Slick/slick.js"  type="text/javascript" charset="utf-8"></script>

    @*Styles.Render("~/Content/css/main")*@
    @Styles.Render("~/Content/css")
    <link rel="stylesheet" href="~/Content/Styles/normalize.min.css">
    @{ Html.RenderAction("GetTheme", "Home"); }
    @Styles.Render("~/Content/css/slick")

    @RenderSection("headContent", false)
</head>
<body>
    <header>
        <div class="col-12 d-flex flex-column flex-lg-row justify-content-between align-items-center title-box">
            <div class="d-flex align-items-center mx-sm-3 my-2">
                <img src="/Content/Images/logo.svg">
                <div class="d-flex flex-column align-items-center ml-2 logo">
                    <a style="text-decoration: none;" href="@Url.Action("Index", "Home")">
                        <h1 class="mb-n3">KINOTIKI</h1>
                        <span>library of movies</span>
                    </a>
                </div>
            </div>
            <div class="d-flex flex-row align-items-center justify-content-center w-100 my-2">
                <input class="w-100 search" placeholder="Search for a movie, TV-Show...">
                <div style="margin-left: -45px;">
                    <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" viewBox="0 0 24 24"><path d="M23.832 19.641l-6.821-6.821c2.834-5.878-1.45-12.82-8.065-12.82-4.932 0-8.946 4.014-8.946 8.947 0 6.508 6.739 10.798 12.601 8.166l6.879 6.879c1.957.164 4.52-2.326 4.352-4.351zm-14.886-4.721c-3.293 0-5.973-2.68-5.973-5.973s2.68-5.973 5.973-5.973c3.294 0 5.974 2.68 5.974 5.973s-2.68 5.973-5.974 5.973z" /></svg>
                </div>
            </div>
            <div class="d-flex my-2">
                @{
                    if (User.Identity.IsAuthenticated)
                    {
                        Html.RenderAction("GetInformationSideMenuBox", "Account");
                    }
                    else
                    {
                        Html.RenderAction("LoginPartial", "Account");
                    }
                }
            </div>
        </div>

        <div class="col-12 justify-content-between align-items-center menu-box">
            <div class="d-flex flex-wrap align-items-center">
                <a href="@Url.Action("Index", "Home")" class="pages-link text-uppercase m-3">Home</a>
                <a href="@Url.Action("UnderConstruction", "Home")" class="pages-link text-uppercase m-3">Movie</a>
                <a href="@Url.Action("UnderConstruction", "Home")" class="pages-link text-uppercase m-3">Gallery</a>
                <a href="@Url.Action("UnderConstruction", "Home")" class="pages-link text-uppercase m-3">Pages</a>
                <a href="@Url.Action("UnderConstruction", "Home")" class="pages-link text-uppercase m-3">News</a>
                <a href="@Url.Action("UnderConstruction", "Home")" class="pages-link text-uppercase m-3">Contact</a>
            </div>
        </div>
    </header>

    @RenderBody()

